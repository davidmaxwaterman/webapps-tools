#! /bin/bash

token="TOKEN";

repo=$1;

echo Repo: $repo;

#owner="davidmaxwaterman";
owner="01org";

# get release from filename
release=$2;

echo Release: $release;

json='{"'tag_name'":"'$release'","'prerelease'":true}';

curl -s -d $json -H "Authorization: token $token" "https://api.github.com/repos/$owner/$repo/releases"
